N_Entero=int(input("Ingresa un numero: "))
N_Flotante=float(input("Ingresa un numero decimal: "))
V_Cadena=str(input("Ingresa tu nombre: "))
Edad=int(input("Ingresa tu edad: "))
Es_Mayor_Edad= Edad >= 18
if Es_Mayor_Edad:
    print("Eres mayor de edad")
else:
    print("Eres menor de edad")

B_Cadena = input("Ingresa un Numero para sumarlo:  ")
numero_convert =int (B_Cadena)
resultado = numero_convert + 789
print(f"El resultado de la suma es: {resultado}")

Text= (input("Ingresa un numero decimal para multiplicarlo: "))
print(Text)
text_floating= float(Text)
Text_Cadena = text_floating*2
print(f"El resultado de la multiplicacion es: {Text_Cadena}")

String= input("Ingresa una frase: ").upper()
print(String)
def convertir_a_int(String):
    try:
        int(String)
        return True
    except ValueError:
        return False
resultado= convertir_a_int(String)
print("se puede convertir a entero?", resultado)
