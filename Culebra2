String= ("hola a todos los estudiantes del 2025").upper()
print(String)
def convertir_a_int(String):
    try:
        int(String)
        return True
    except ValueError:
        return False
resultado= convertir_a_int(String)
print("se puede convertir a entero?", resultado)
